{
    "name": "guirod/atoum-bundle",
    "description": "Bundle around atoum unit testing framework",
    "type": "symfony-bundle",
    "keywords": ["atoum", "test", "unit testing"],
    "homepage": "https://github.com/guirod/AtoumBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Florian Lonqueu-Brochard",
            "email": "dev@florianlb.fr"
        },
        {
            "name": "guirod"
        }
    ],
    "require": {
        "php": "^5.6.0 || ^7.0.0 <7.4.0",
        "symfony/framework-bundle": ">=3.4.0|^4.0",
        "symfony/console": ">=3.4.0|^4.0",
        "symfony/finder": ">=3.4.0|^4.0",
        "symfony/dom-crawler": ">=3.4.0|^4.0",
        "symfony/css-selector": ">=3.4.0|^4.0",
        "atoum/atoum": "^3.3.0",
        "fzaninotto/faker": "1.*"
    },
    "require-dev": {
        "symfony/form": "3.*"
    },
    "autoload": {
        "psr-0": {
            "atoum\\AtoumBundle": ""
        }
    },
    "target-dir": "guirod/AtoumBundle",
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5-dev"
        }
    }
}
